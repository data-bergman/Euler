    "import math\n",
    "p=1\n",
    "s={}\n",
    "s[0]=2\n",
    "s[1]=3\n",
    "m=4\n",
    "n=0\n",
    "y=5\n",
    "while m<20000000:\n",
    "    while s[n] <= math.sqrt(m) and m % s[n] != 0:\n",
    "        n += 1\n",
    "        if s[n] > math.sqrt(m):\n",
    "            p = p+1\n",
    "            s[p] = m\n",
    "            y += m\n",
    "            print(p,s[p])\n",
    "    m = m + 1\n",
    "    n=0\n",
    "print(y)"
   ]
  }
